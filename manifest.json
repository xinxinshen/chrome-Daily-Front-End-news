{
  "manifest_version": 2,

  "name": "Front End News",
  "description": "Front End News every day",
  "author": "fengshangwuqi, fengshangwuqi@gmail.com",
  "version": "1.0",
  "icons": {
    "128": "images/icon.png"
  },

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": [
      "background/utils.js",
      "background/API.js",
      "background/News.js",
      "background/background.js"
    ]
  },

  "permissions": ["webRequest"]
}
